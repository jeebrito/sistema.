
#include <stdio.h>
int main() {     
int M=1,N=1,c, 
total=0;
while(M>0&&N>0){ 
  scanf("%d%d", &M,&N);    
if(M>N&&M>0&&N>0){ //analizar as codiçães para o programa fucionar    
c=N;        
while(c<=M){            
printf("%d ", c);            
total=total+c;//adcionar cada numeros as variavel total            
c=c+1;        
}    
}    
else if(N>M&&M>0&&N>0){    
c=M;        
while(c<=N){            
printf("%d ", c);//exebir a senquecias            
total=total+c;            
c=c+1;        
}    
}     
if(M>0&&N>0)    
printf("Sum=%d\n", total);    
total=0;}    
return 0;
